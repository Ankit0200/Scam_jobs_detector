
{
  "manifest_version": 3,
  "name": "Job Scam Analyzer",
  "version": "1.0",
  "description": "Analyze job listings and detect potential scams.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
